{
    "name": "arrow",
    "version": "1.0.0",
    "description": "A modern JavaScript utility library for complex data structures",
    "main": "build/arrow-alpha.js",
    "scripts": {
        "lint": "eslint ./**",
        "lint:fix": "eslint ./** --fix",
        "lint:src": "eslint ./src",
        "lint:src:fix": "eslint ./src --fix",
        "lint:fix-cache": "eslint ./** --fix --cache",
        "test": "jest ./specs/1.x.x/scripts/array --config=jest.config.js --detectOpenHandles --watchAll",
        "test:spec": "jest ./specs/1.x.x/scripts/array/order.spec.js --config=jest.config.js --detectOpenHandles --watch",
        "push": "git push",
        "babel": "babel --presets es2015 ./src/index.js -o build/arrow-alpha.js",
        "build": "webpack --colors",
        "build:watch": "webpack -d --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MenaiAla/arrow.git"
    },
    "keywords": [
        "javascript",
        "arrays",
        "algorithm"
    ],
    "author": "Menai Ala Eddine",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MenaiAla/arrow/issues"
    },
    "homepage": "https://github.com/MenaiAla/arrow#readme",
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/plugin-transform-modules-commonjs": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "babel-jest": "^26.1.0",
        "eslint": "^7.7.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-plugin-import": "^2.22.0",
        "jest": "^26.1.0",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "webpack": "^4.44.1"
    }
}